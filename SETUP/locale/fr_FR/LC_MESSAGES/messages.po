#: quiz/generic/data/qd_formatting4.inc:73
msgid "Poem marked incorrectly"
msgstr ""

#: quiz/generic/data/qd_formatting4.inc:74
msgid "Mark poetry with the markup <tt>/* */</tt> to preserve the line breaks."
msgstr ""

#: quiz/generic/data/qd_formatting4.inc:78
msgid "Block quote marked incorrectly"
msgstr ""

#: quiz/generic/data/qd_formatting4.inc:79
msgid ""
"Mark block quotes with the markup <tt>/# #/</tt> to allow the text to be "
"rewrapped."
msgstr ""

#: quiz/generic/data/qd_formatting4.inc:83
msgid "List not marked"
msgstr "Listes non marquées"

#: quiz/generic/data/qd_formatting4.inc:84
msgid "Mark lists with the markup <tt>/* */</tt>."
msgstr "Marquez les listes avec le marquage <tt>/* */</tt>."

